{
    "name": "pewpewyou/nashor-scraper",
    "type": "library",
    "description": "Nashor is a fast high-level web crawling and web scraping tool for OP.GG",
    "keywords": [
        "league",
        "legends",
        "opgg",
        "nashor-scraper"
    ],
    "homepage": "https://github.com/pewpewyou/nashor-scraper",
    "license": "MIT",
    "authors": [
        {
            "name": "Ernest Hernandez",
            "email": ":htc.ernest.hernandez@gmail.com",
            "homepage": "ernest.gallery",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0",
        "guzzlehttp/guzzle": "^6.1.1",
        "paquettg/php-html-parser": "^1.7",
		"fabpot/goutte": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3",
        "friendsofphp/php-cs-fixer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "NashorScraper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NashorScraper\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
